namespace ConsoleApp1_c_
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter numbers :");
            int num_1 = Convert.ToInt32(Console.ReadLine());

            if (num_1 >= 1 && num_1 <= 10)
            {

                Console.WriteLine("valid");
            }
            else
            {
                Console.WriteLine("invalid");
            }
        }
    }
}
